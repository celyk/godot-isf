code = "// Godot Shader generated from ISF (Interactive Shader Format)

shader_type canvas_item;

#include \"res://addons/godot-isf/include/ISF.gdshaderinc\"


// INPUTS 
uniform sampler2D inputImage : source_color;


// IMPORTED 
uniform sampler2D blendImage : source_color;


// PASSES 


/*{
	\"DESCRIPTION\": \"demonstrates the use of multiple image-type inputs\",
	\"CREDIT\": \"by zoidberg\",
	\"ISFVSN\": \"2.0\",
	\"CATEGORIES\": [
		\"TEST-GLSL FX\"
	],
	\"INPUTS\": [
		{
			\"NAME\": \"inputImage\",
			\"TYPE\": \"image\"
		}
	],
	\"IMPORTED\": {
		\"blendImage\": {
			\"PATH\": \"Hexagon.tiff\"
		}
	}
}*/

void main()
{
	vec4		srcPixel = IMG_NORM_PIXEL(inputImage, isf_FragNormCoord);
	vec4		blendPixel = IMG_NORM_PIXEL(blendImage, isf_FragNormCoord);
	
	gl_FragColor = (srcPixel + blendPixel)/2.0;
}
"

